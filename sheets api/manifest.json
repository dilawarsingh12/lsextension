{
    "manifest_version": 2,
  
    "name": "leetsheet",
    "description": "save your questions",
    "version": "1.0",     
    "browser_action": {      
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "https://leetsheet.herokuapp.com/update",
      "https://leetsheet.herokuapp.com/create",
      "https://leetsheet.herokuapp.com/validate",
      "http://127.0.0.1:8000/update",
      "http://127.0.0.1:8000/create",
      "http://127.0.0.1:8000/validate",      
      "tabs",
      "storage"      
    ],
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js; object-src 'self'"
    
}